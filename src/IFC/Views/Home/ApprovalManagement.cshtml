                        <th>Approval request ID</th>
                        <th>Request Type</th>
                        <th>Initiated On</th>
                        <th > Status</th>
                        
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>1</td>
                        <td>CBT</td>
                        <td>100</td>
                        <td>700</td>
                        <td><i class="fa fa-edit"></i><i class="fa fa-view"></i><i class="fa fa-trashdotnet watch --project src/IFC"></i></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</section>